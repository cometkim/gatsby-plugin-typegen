    "@types/graphql": "^14.5.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.7.3",
    "@types/normalize-path": "^3.0.0",
    "babel-preset-gatsby-package": "^0.2.3",
    "gatsby": "^2.15.6",
    "typescript": "^3.7.4",
