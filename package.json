    "@graphql-codegen/typescript-resolvers": "^1.12.1",
    "@graphql-toolkit/code-file-loader": "^0.9.7",
    "@graphql-toolkit/common": "^0.9.7",
    "@graphql-toolkit/core": "^0.9.7",
    "@graphql-toolkit/json-file-loader": "^0.9.7",
