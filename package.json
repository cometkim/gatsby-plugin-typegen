{
  "name": "gatsby-plugin-typegen",
  "description": "Gatsby plugin that watch your static/page queries and automatically generates TypeScript/Flow definitions.",
  "version": "2.0.1",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Hyeseong Kim",
    "email": "cometkim.kr@gmail.com",
    "url": "https://github.com/cometkim"
  },
  "homepage": "https://www.gatsbyjs.org/packages/gatsby-plugin-typegen/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/gatsby-plugin-typegen.git"
  },
  "bugs": {
    "url": "https://github.com/cometkim/gatsby-plugin-typegen/issues",
    "email": "cometkim.kr@gmail.com"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "typescript",
    "flow",
    "codegen"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx",
    "test": "jest",
    "tsc": "tsc --emitDeclarationOnly --skipLibCheck",
    "flowtype": "cp -f src/types.ts src/types.js && babel src/types.js -d . --plugins module:@babel/plugin-transform-flow-comments",
    "build": "babel src -d . --extensions .ts,.tsx",
    "prepublish": "yarn build && yarn tsc && yarn flowtype"
  },
  "resolutions": {
    "acorn": "7.1.0",
    "minimist": "1.2.5",
    "@babel/plugin-transform-named-capturing-groups-regex": "7.4.4"
  },
  "peerDependencies": {
    "gatsby": ">=2.0.0",
    "graphql": "^0.13.0 || ^14.0.0",
    "react": "^16.4.2"
  },
  "dependencies": {
    "@cometjs/core": "^0.0.3",
    "@graphql-codegen/core": "^1.13.2",
    "@graphql-codegen/flow": "^1.13.2",
    "@graphql-codegen/flow-operations": "^1.13.2",
    "@graphql-codegen/flow-resolvers": "^1.13.2",
    "@graphql-codegen/typescript": "^1.13.2",
    "@graphql-codegen/typescript-operations": "^1.13.2",
    "@graphql-codegen/typescript-resolvers": "^1.13.2",
    "@graphql-toolkit/common": "^0.10.3",
    "@graphql-toolkit/core": "^0.10.3",
    "@graphql-toolkit/graphql-tag-pluck": "^0.10.3",
    "async": "^3.2.0",
    "common-tags": "^1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-flow-comments": "^7.8.3",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.8.3",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@types/async": "^3.2.0",
    "@types/common-tags": "^1.8.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.20",
    "all-contributors-cli": "^6.14.1",
    "babel-jest": "^25.3.0",
    "babel-preset-gatsby-package": "^0.3.1",
    "eslint": "^6.8.0",
    "gatsby": "^2.20.22",
    "graphql": "^15.0.0",
    "jest": "^25.3.0",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0"
  }
}
