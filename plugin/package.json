{
  "name": "gatsby-plugin-typegen",
  "description": "Gatsby plugin to play with extream type-safety",
  "version": "2.2.4",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Hyeseong Kim",
    "email": "hey@hyeseong.kim",
    "url": "https://github.com/cometkim"
  },
  "homepage": "https://www.gatsbyjs.org/packages/gatsby-plugin-typegen/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/gatsby-plugin-typegen.git"
  },
  "bugs": {
    "url": "https://github.com/cometkim/gatsby-plugin-typegen/issues",
    "email": "hey@hyeseong.kim"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "graphql",
    "codegen",
    "typescript",
    "flow"
  ],
  "scripts": {
    "test": "jest",
    "build": "concurrently \"yarn:build:*\"",
    "build:js": "babel src -d . --extensions .ts,.tsx",
    "build:dts": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:flow": "babel src/types.js.flow -x .flow -d . --plugins module:@babel/plugin-transform-flow-comments",
    "develop": "babel src -d . --extensions .ts,.tsx --watch",
    "prepack": "yarn build"
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "gatsby-node.js",
    "gatsby-utils.js",
    "gatsby-utils.d.ts",
    "types.js",
    "types.d.ts",
    "types.js.flow",
    "internal",
    "services"
  ],
  "peerDependencies": {
    "gatsby": "^4.0.0",
    "graphql": "^15.0.0"
  },
  "dependencies": {
    "@graphql-codegen/core": "^2.5.1",
    "@graphql-codegen/flow": "^2.2.7",
    "@graphql-codegen/flow-operations": "^2.2.7",
    "@graphql-codegen/flow-resolvers": "^2.2.8",
    "@graphql-codegen/typescript": "^2.4.7",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@graphql-codegen/typescript-resolvers": "^2.5.4",
    "@graphql-tools/utils": "^8.6.2",
    "xstate": "^4.30.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-flow-comments": "^7.16.7",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/runtime": "^7.17.2",
    "@types/common-tags": "^1.8.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.4.1",
    "@types/jest-when": "^3.5.0",
    "@types/node": "^17.0.21",
    "@xstate/cli": "^0.1.4",
    "babel-preset-gatsby-package": "^2.9.0",
    "common-tags": "^1.8.2",
    "concurrently": "^7.0.0",
    "gatsby": "^4.9.3",
    "graphql": "15.8.0",
    "jest": "^27.5.1",
    "jest-when": "^3.5.1",
    "typescript": "^4.6.2"
  }
}
