{
  "name": "gatsby-plugin-typegen",
  "description": "Gatsby plugin to play with extream type-safety",
  "version": "2.2.4",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Hyeseong Kim",
    "email": "hey@hyeseong.kim",
    "url": "https://github.com/cometkim"
  },
  "homepage": "https://www.gatsbyjs.org/packages/gatsby-plugin-typegen/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/gatsby-plugin-typegen.git"
  },
  "bugs": {
    "url": "https://github.com/cometkim/gatsby-plugin-typegen/issues",
    "email": "hey@hyeseong.kim"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "graphql",
    "codegen",
    "typescript",
    "flow"
  ],
  "scripts": {
    "test": "jest",
    "build": "concurrently \"build:*\"",
    "build:js": "babel src -d . --extensions .ts,.tsx",
    "build:dts": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:flow": "babel src/types.js.flow -x .flow -d . --plugins module:@babel/plugin-transform-flow-comments",
    "develop": "babel src -d . --extensions .ts,.tsx --watch",
    "prepack": "yarn build"
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "gatsby-node.js",
    "gatsby-utils.js",
    "gatsby-utils.d.ts",
    "types.js",
    "types.d.ts",
    "types.js.flow",
    "internal",
    "services"
  ],
  "peerDependencies": {
    "gatsby": ">=2.2.0",
    "react": "^16.4.2 || ^17.0.0"
  },
  "dependencies": {
    "@cometjs/core": "^1.2.0",
    "@graphql-codegen/core": "^1.17.9",
    "@graphql-codegen/flow": "^1.19.0",
    "@graphql-codegen/flow-operations": "^1.18.6",
    "@graphql-codegen/flow-resolvers": "^1.17.13",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "xstate": "^4.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-flow-comments": "^7.12.1",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-typescript": "^7.13.0",
    "@cometjs/eslint-plugin": "^1.1.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.23",
    "@types/jest-when": "^2.7.2",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby-package": "^1.4.0",
    "concurrently": "^6.1.0",
    "eslint": "^7.26.0",
    "gatsby": "^3.4.2",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "jest-when": "^3.3.1",
    "redux": "^4.1.0",
    "typescript": "^4.2.4"
  }
}
